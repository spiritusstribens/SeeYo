<!DOCTYPE html>
<html>
<head>
	<title><%= @page_title || 'SeeYo' %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<p style='background-color:#CFC; text-align:center; border-radius:7px'><%= flash[:notice] %></p>
<p style='background-color:#FCC; text-align:center; border-radius:7px'><%= flash[:alert] %></p>
<%= yield %>

</body>
</html>
